{
  "name": "lms-sanpablo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "build": "prisma generate --no-engine && next build",
    "build:safe": "npm run lint:fix && prisma generate --no-engine && next build",
    "build:vercel": "prisma generate --no-engine && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --max-warnings 50",
    "lint:check": "next lint --max-warnings 50",
    "lint:fix": "next lint --fix --max-warnings 100",
    "type-check": "tsc --noEmit",
    "precheck": "npm run type-check && npm run lint:check",
    "seed": "tsx scripts/seed.ts",
    "cleanup": "tsx scripts/cleanup-database.ts",
    "optimize": "node scripts/optimize-performance.js",
    "clean": "rm -rf .next && rm -rf node_modules && rm -f package-lock.json",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "npx prisma studio",
    "setup-safe": "node scripts/prevent-build-errors.js",
    "fix-smart": "node scripts/fix-smart.js",
    "fix-critical": "node scripts/fix-critical-errors.js",
    "fix-massive": "node scripts/fix-errors.js",
    "fix-nextjs15": "node scripts/fix-nextjs15-params.js",
    "fix-all": "powershell -ExecutionPolicy Bypass -File scripts/fix-all.ps1",
    "fix-all-bash": "bash scripts/fix-all.sh",
    "fix-all-errors": "node scripts/fix-build-errors-final.js",
    "build-safe": "npm run fix-critical && npm run build",
    "quick-fix": "npm run fix-smart && npm run build-safe",
    "clean-all": "powershell -Command \"Remove-Item -Path '.next' -Recurse -Force -ErrorAction SilentlyContinue; Remove-Item -Path 'node_modules' -Recurse -Force -ErrorAction SilentlyContinue; Remove-Item -Path 'package-lock.json' -Force -ErrorAction SilentlyContinue\"",
    "fresh-install": "npm run clean-all && npm cache clean --force && npm install && npx prisma generate",
    "dev-safe": "npm run fix-critical && npm run dev",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate --no-engine && next build",
    "full-setup": "npm run setup-safe && npm run fix-all-errors && npm run build:safe"
  },
