{
  "buildCommand": "prisma generate --no-engine && next build",
  "devCommand": "npm run dev",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 30
    },
    "app/**/*.js": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "PRISMA_CLI_QUERY_ENGINE_TYPE": "binary",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "PRISMA_CLI_QUERY_ENGINE_TYPE": "binary",
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  }
} 